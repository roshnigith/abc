#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
char s[100];
int i,n,flag=0;
printf("\n Enter the string");
scanf("%s",&s);
n=strlen(s);
for(i=0;i<n;i++)
{
 if(s[i]!='a')
 flag=1;
}
if(flag==0)
printf("String has all a's");
else
printf("String does not ahve all a's");
getch();
}
